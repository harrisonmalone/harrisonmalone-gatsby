{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy-post": "git add . && git commit -m 'new post' && npm run deploy",
    "deploy-edited-post": "git commit -am 'edited post' && npm run deploy",
    "deploy": "gatsby build && netlify deploy --prod --dir=public"
  },
  "dependencies": {
    "gatsby": "^2.21.33",
    "gatsby-plugin-feed": "^2.5.10",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-remark-prismjs": "^3.5.1",
    "gatsby-source-filesystem": "^2.3.3",
    "gatsby-transformer-remark": "^2.8.8",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@iconify/icons-mdi": "^1.0.105",
    "@iconify/react": "^1.1.3",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
